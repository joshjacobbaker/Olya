{
  "name": "olya",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "start": "turbo run start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "eslint-config-custom": "workspace:*",
    "pnpm": "^7.8.0",
    "postcss": "^8.4.14",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "turbo": "^1.4.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.8.0",
  "resolutions": {
    "@types/react": "<18.0.0",
    "@types/react-dom": "<18.0.0"
  }
}
